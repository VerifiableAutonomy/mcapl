@using = mcapl

target = ail.util.AJPF_w_AIL
target.args = ${mcapl}/src/examples/gwendolen/auctions/coalition/CoalitionAuction4Bidders.ail,${mcapl}/src/examples/gwendolen/auctions/auctions.psl,1

